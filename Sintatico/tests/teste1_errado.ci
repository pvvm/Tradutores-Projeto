float calcula_imc(float list valores) {
    float peso;
    float altura;
    altura = ?valores;
    valores = %valores
    peso = ?valores;

    return(peso / (altura * altura));

}

int classifica_imc(float imc) {
    if (0 < imc && imc < 25) {
        writeln("De magro ate peso normal");
    } else {
        writeln/*aqui*/("Com sobrepeso ou com obesidade");
    }
}

