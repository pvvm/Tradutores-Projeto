// Funcao que imprime a lista
int oi;

int _write_list(int list lista, int tamanho) {
    int i;
    int header;
    for(i = 0; i < tamanho; i = i + 1) {
        header = ?lista;    // Testando comentario
        lista = %lista;
        write(header);
        write(" ");
    }
    return 1;
}

// Funcao que soma e subtrai 5

int soma(int a, int b) {
    return a+b*5;
}

