    if(imc < 18..5) {// testando
        writeln(Abaixo do peso");
    } else if (18.5 <= imc && imc < 25) {
        writeln("Peso normal");
    } else if (25 <= imc & imc < 30.5.6) {
        writeln("Sobrepeso");/*
        testando
        comentario
        multilinha*/
    } else {
        writeln/*aqui*/("Obesidade de grau 1 para cima");
    }
}

